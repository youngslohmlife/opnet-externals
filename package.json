{
  "name": "opnet-externals",
  "type": "module",
  "version": "0.1.0",
  "description": "@public decorator for OP_NET contracts",
  "author": "flex",
  "license": "MIT",
  "scripts": {
    "build:transform": "tsc -p ./transform",
    "build": "npm-run-all build:transform asbuild:untouched",
    "build:test": "asc --transform ./transform test/index.ts",
    "asbuild:untouched": "asc assembly/index.ts --target debug",
    "asbuild:optimized": "asc assembly/index.ts --target release",
    "asbuild": "yarn asbuild:untouched && yarn asbuild:optimized",
    "prettier:ts": "prettier --write src.ts/**/*.ts",
    "prettier:as": "prettier --write assembly/**/*.ts"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^18.11.13",
    "assemblyscript": "^0.27.29",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "assemblyscript": "$assemblyscript"
  },
  "dependencies": {
    "@btc-vision/btc-runtime": "^1.1.3",
    "as-container": "^0.8.0",
    "as-string-sink": "^0.5.3",
    "as-variant": "^0.4.1",
    "as-virtual": "^0.2.0",
    "lodash.clonedeep": "^4.5.0",
    "ts-mixer": "^6.0.2",
    "visitor-as": "^0.11.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngslohmlife/opnet-externals.git"
  },
  "keywords": [
    "assemblyscript",
    "externals",
    "OP_NET"
  ]
}
